{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/admin/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/ws",
      "dest": "server.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/src/public/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/src/public/sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/public/$1"
    }
  ]
}
